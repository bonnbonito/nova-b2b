{
  "name": "nova-b2b",
  "version": "1.0.0",
  "description": "**Contributors:** bonnbonito **Requires at least:** 5.4 **Tested up to:** 6.2 **Requires PHP:** 7.2",
  "scripts": {
    "preview": "npm-run-all --parallel sync tailwindwatch",
    "sync": "browser-sync start --config bs-config.js",
    "tailwindwatch": "tailwindcss -i ./assets/input.css -o ./assets/css/dist/output.css --watch --minify",
    "tailwindbuild": "tailwindcss -i ./assets/input.css -o ./assets/css/dist/output.css --minify"
  },
  "devDependencies": {
    "@tailwindcss/forms": "^0.5.7",
    "@tailwindcss/typography": "^0.5.10",
    "browser-sync": "^2.29.3",
    "daisyui": "^4.4.20",
    "npm-run-all": "^4.1.5",
    "tailwindcss": "^3.4.3",
    "tailwindcss-animate": "^1.0.7"
  },
  "dependencies": {
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.1",
    "react-datepicker": "^7.3.0",
    "tailwind-merge": "^2.3.0"
  }
}
